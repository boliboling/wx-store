.dialog-wrap{

}
.dialog-wrap-dn{
	display: none;
}
.dialog-bg{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,0.3);
}
.dialog-main{
	position: fixed;
	left: 50%;
	top: 50%;
	width: 250px;
	background-color: #fff;
	transform: translate(-50%,-50%);
	border-radius: 5px;
}
.dialog-con{
	text-align: center;
	line-height: 15px;
	height: 100px;
	line-height: 100px;
	color: #9E9E9E;
	border-bottom: 1px solid #9E9E9E;
}
.dialog-btn-wrap{
	display: flex;
	flex-direction: row;
	justify-content: space-around;
}
.dialog-btn{
	width: 100%;
	color: #9E9E9E;
	line-height: 40px;
	text-align: center;
}